{
  "name": "pemike-api",
  "version": "1.0.0",
  "description": "A transport booking and management system",
  "main": "dist/app.js",
  "type": "module",
  "scripts": {
    "build": "tsc --build",
    "dev": "nodemon  --exec tsx -r dotenv/config src/app.ts",
    "start": "node dist/app.js",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:seed": "tsx -r dotenv/config prisma/seeders/trip.seeder.ts",
    "db:studio": "prisma studio"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cysteine12/Pemike-API.git"
  },
  "author": "Anettcom Technologies",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Cysteine12/Pemike-API/issues"
  },
  "homepage": "https://github.com/Cysteine12/Pemike-API#readme",
  "dependencies": {
    "@prisma/client": "^6.4.1",
    "axios": "^1.8.4",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.18.1",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.12.0",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.10.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "tsx": "^4.19.3",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/express-rate-limit": "^5.1.3",
    "@types/express-session": "^1.18.1",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.13.9",
    "@types/node-cache": "^4.1.3",
    "@types/nodemailer": "^6.4.17",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/uuid": "^10.0.0",
    "@types/winston": "^2.4.4",
    "dotenv": "^16.4.7",
    "nodemon": "^3.1.9",
    "prisma": "^6.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
